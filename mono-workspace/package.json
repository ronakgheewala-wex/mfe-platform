{
  "name": "mono-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:host": "ng serve host-app -o --port 4200",
    "start:remoteapp": "ng serve remote-app -o --port 4201",
    "start:all": "npm run start:remoteapp & npm run start:host",
    "build:host": "ng build host-app",
    "build:remoteapp": "ng build remote-app",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "^21.1.2",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@primeuix/themes": "^2.0.3",
    "primeicons": "^7.0.0",
    "primeng": "^21.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-architects/module-federation": "^20.0.0",
    "@angular-devkit/build-angular": "^21.0.5",
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.0",
    "jsdom": "^27.1.0",
    "ngx-build-plus": "^20.0.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
